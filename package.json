{
  "name": "nextjs-argon-dashboard",
  "version": "1.1.0",
  "author": "Creative Tim",
  "license": "MIT",
  "homepage": "https://demos.creative-tim.com/nextjs-argon-dashboard/",
  "description": "NextJS version of Argon Dashboard React by Creative Tim",
  "bugs": {
    "url": "https://github.com/creativetimofficial/nextjs-argon-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/nextjs-argon-dashboard.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next/ out/ node_modules/.cache/",
    "clean:all": "rm -rf node_modules/ .next/ out/ coverage/ .nyc_output/",
    "install:clean": "pnpm run clean:all && pnpm install && pnpm run dev",
    "build:analyze": "ANALYZE=true next build",
    "build:scss": "sass assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css --style=compressed",
    "compile:scss": "sass assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
    "minify:scss": "sass --style=compressed assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.min.css",
    "map:scss": "sass --source-map assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.2",
    "@tanstack/react-query": "^5.86.0",
    "@tanstack/react-query-devtools": "^5.86.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "axios": "^1.11.0",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "moment": "2.29.1",
    "next": "14.2.5",
    "nouislider": "14.6.3",
    "react": "18.3.1",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "18.3.1",
    "reactstrap": "8.9.0",
    "sass": "^1.70.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}