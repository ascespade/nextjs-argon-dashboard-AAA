{
  "name": "nextjs-argon-dashboard",
  "version": "1.1.0",
  "author": "Creative Tim",
  "license": "MIT",
  "homepage": "https://demos.creative-tim.com/nextjs-argon-dashboard/",
  "description": "NextJS version of Argon Dashboard React by Creative Tim",
  "bugs": {
    "url": "https://github.com/creativetimofficial/nextjs-argon-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/nextjs-argon-dashboard.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next/ out/ node_modules/.cache/",
    "clean:all": "rm -rf node_modules/ .next/ out/ coverage/ .nyc_output/",
    "install:clean": "pnpm run clean:all && pnpm install && pnpm run dev",
    "build:analyze": "ANALYZE=true next build",
    "build:scss": "sass assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css --style=compressed",
    "compile:scss": "sass assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
    "minify:scss": "sass --style=compressed assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.min.css",
    "map:scss": "sass --source-map assets/scss/nextjs-argon-dashboard.scss assets/css/nextjs-argon-dashboard.css",
    "pre-commit": "lint-staged",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:ci"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@supabase/supabase-js": "^2.57.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.6",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.460.0",
    "moment": "^2.30.1",
    "next": "^15.1.0",
    "next-intl": "^3.22.4",
    "nouislider": "^15.7.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-datetime": "^3.2.0",
    "react-dom": "^18.3.1",
    "reactstrap": "^9.2.2",
    "sass": "^1.83.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@next/eslint-plugin-next": "^15.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
